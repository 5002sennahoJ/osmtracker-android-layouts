<layouts>
    <layout name="root">
        <row>
            <button type="picture" icon="logo_camera"/>
            <button type="textnote" icon="logo_textnote"/>
            <button type="voicerec" icon="logo_voice"/>
        </row>
        <row>
            <button type="tag" label="Ciclovía Inicio" icon="movilidad_icons/ciclovía-inicio.png" />
            <button type="tag" label="Ciclo Carril Inicio" icon="movilidad_icons/ciclo-carril-inicio.png"/>
            <button type="tag" label="Sharrow Inicio" icon="movilidad_icons/sharrow-inicio.png"/>
        </row>
        <row>
            <button type="tag" label="Ciclovía Fin" icon="movilidad_icons/ciclovía-fin.png" />
            <button type="tag" label="Ciclo Carril Fin" icon="movilidad_icons/ciclo-carril-fin.png"/>
            <button type="tag" label="Sharrow Fin" icon="movilidad_icons/sharrow-fin.png"/>
        </row>
        <row>
            <button type="tag" label="Barra Confinamiento Inicio" icon="movilidad_icons/barra-confinamiento-inicio.png" />
            <button type="tag" label="Barra Confinamiento Fin" icon="movilidad_icons/barra-confinamiento-fin.png" />
            <button type="page" label="Elementos Individuales" targetlayout="elementos_individuales"/>
        </row>
    </layout>
    <layout name="elementos_individuales">
        <row>
            <button type="picture" icon="logo_camera"/>
            <button type="textnote" icon="logo_textnote"/>
            <button type="voicerec" icon="logo_voice"/>
        </row>
        <row>
            <button type="tag" label="Bici Caja" icon="movilidad_icons/bici-caja.png"/>
            <button type="tag" label="Bolardo" icon="movilidad_icons/bolardo.png"/>
            <button type="tag" label="Trayectoria" icon="movilidad_icons/trayectoria.png"/>
        </row>
        <row>
            <button type="tag" label="Ceda Ciclista" icon="movilidad_icons/ceda-ciclista.png"/>
            <button type="tag" label="Parqueo" icon="movilidad_icons/parqueo.png"/>
        </row>
    </layout>

</layouts>
